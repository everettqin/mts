                <itemizedlist>
                    <listitem>
                            <para>#define <link linkend="d1/d9f/group___t_i_m___clock___division___c_k_d_1ga88691a07b3976791977d280045b3c850">TIM_CKD_DIV1</link> ((uint16_t)0x0000)</para>
