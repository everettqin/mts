            <para>
                <itemizedlist>
                    <listitem>
                            <para>#define <link linkend="d7/d0e/group___u_s_a_r_t___interrupt__definition_1gae607c28a462c224c575b7541dc4f7067">USART_IT_PE</link> ((uint16_t)0x0028)</para>
                    </listitem>
