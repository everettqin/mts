            <para>
                <itemizedlist>
                    <listitem>
                            <para>#define <link linkend="de/d24/group___r_t_c___alarms___definitions_1ga8d6daed1177365822e9f4e443ec394c7">RTC_Alarm_A</link> ((uint32_t)0x00000100)</para>
                    </listitem>
                </itemizedlist>
            </para>
            <para>
                <itemizedlist>
                    <listitem>
                            <para>#define <link linkend="de/d24/group___r_t_c___alarms___definitions_1gaf51f54c28f0052d97a8764741218d372">RTC_Alarm_B</link> ((uint32_t)0x00000200)</para>
                    </listitem>
                </itemizedlist>
            </para>
            <para>
                <itemizedlist>
                    <listitem>
                            <para>#define <link linkend="de/d24/group___r_t_c___alarms___definitions_1gab101ad18b0bcde557eb8caac469d7725">IS_RTC_ALARM</link> (((ALARM) == RTC_Alarm_A) || ((ALARM) == RTC_Alarm_B))</para>
                    </listitem>
                </itemizedlist>
            </para>
            <para>
                <itemizedlist>
                    <listitem>
                            <para>#define <link linkend="de/d24/group___r_t_c___alarms___definitions_1gae6b65274a46fc3a9f64eb0c9ed4c13e4">IS_RTC_CMD_ALARM</link> (((ALARM) &amp; (RTC_Alarm_A | RTC_Alarm_B)) != (uint32_t)RESET)</para>
                    </listitem>
                </itemizedlist>
            </para>
