            <para>
                <itemizedlist>
                    <listitem>
                            <para>#define <link linkend="d5/d42/group___d_m_a__fifo__status__level_1gaf7ea7373a08730e773cbc048c9965dc2">DMA_FIFOStatus_Full</link> ((uint32_t)0x00000005 &lt;&lt; 3)</para>
