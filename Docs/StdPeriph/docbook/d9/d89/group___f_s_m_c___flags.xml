                <itemizedlist>
                    <listitem>
                            <para>#define <link linkend="d9/d89/group___f_s_m_c___flags_1ga5aadbd5d9f1b6a25bcc1fc6f3bf4c9cc">FSMC_FLAG_RisingEdge</link> ((uint32_t)0x00000001)</para>
